<Window x:Class="ObjectDataProviderSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ObjectDataProviderSample"
        mc:Ignorable="d"
        Title="MainWindow" Height="100" Width="200">
    <StackPanel>
        <Button MinHeight="15" Click="ButtonBase_OnClick"></Button>
        <TextBox x:Name="TextBoxArg1" MinHeight="20"></TextBox>
        <TextBox x:Name="TextBoxArg2" MinHeight="20"></TextBox>
        <TextBox x:Name="TextBoxResult" MinHeight="20"></TextBox>
    </StackPanel>
</Window>
